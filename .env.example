# Copy this file to .env and update with your configuration
# SQLite database (default, file-based, no external dependencies)
DATABASE_URL=sqlite+aiosqlite:///./data/lumina.db

# Chroma vector store settings
CHROMA_PERSIST_DIR=./data/chroma_db
VECTOR_COLLECTION=news_embeddings

# Embedding model (Ollama)
EMBEDDING_MODEL=qwen3-embedding

DEBUG=true
